{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@components": [
        "src/components/index.ts"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@screens": [
        "src/screens/index.ts"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@styles": [
        "src/styles/index.ts"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
    }
  },
  "extends": "expo/tsconfig.base",
}