{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@components": [
        "src/components/index.ts"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@screens": [
        "src/screens/index.ts"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@styles": [
        "src/styles/index.ts"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@routes": [
        "src/routes/index.ts"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@config": [
        "src/config/index.ts"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@utils": [
        "src/utils/index.ts"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@hooks": [
        "src/hooks/index.ts"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@services": [
        "src/services/index.ts"
      ],
      "@mappers/*": [
        "src/mappers/*"
      ],
      "@mappers": [
        "src/mappers/index.ts"
      ],
      "@types/*": [
        "src/@types/*"
      ],
      "@types": [
        "src/@types/index.d.ts"
      ],
    }
  },
  "extends": "expo/tsconfig.base",
}